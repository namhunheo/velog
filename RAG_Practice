    {
      "cell_type": "code",
      "source": [
        "#오버랩을 적용한 청킹\n",
        "def chunk_text(text, chunk_size=500, overlap=100):\n",
        "    chunks = []\n",
        "    start = 0\n",
        "\n",
        "    while start < len(text):\n",
        "        end = start + chunk_size\n",
        "        chunks.append(text[start:end])\n",
        "\n",
        "        #다음 시작 위치를 overlap만큼 뒤로 당김\n",
        "        start += chunk_size - overlap\n",
        "\n",
        "    return chunks\n",
        "\n",
        "\n",
        "documents = chunk_text(text, chunk_size=500, overlap=100)\n",
        "\n",
        "print(\"총 청크 수:\", len(documents))"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "iwIPZxWcDKP6",
        "outputId": "b7756eeb-5421-4093-88ed-3ee0fe974b20"
      },
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "총 청크 수: 100\n"
          ]
        }
      ]
