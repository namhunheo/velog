      "cell_type": "code",
      "source": [
        "def chunk_by_paragraph(text):\n",
        "    paragraphs = text.split(\"\\n\\n\")  # 빈 줄 기준 분할\n",
        "\n",
        "    # 공백 제거 + 빈 문단 제거\n",
        "    paragraphs = [p.strip() for p in paragraphs if p.strip()]\n",
        "\n",
        "    return paragraphs\n",
        "\n",
        "\n",
        "documents = chunk_by_paragraph(text)\n",
        "\n",
        "print(\"총 문단 수:\", len(documents))"
      ],
      "metadata": {
        "colab": {
