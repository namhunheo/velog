      "cell_type": "code",
      "source": [
        "def chunk_text(text, chunk_size=500):\n",
        "    chunks = []\n",
        "    for i in range(0, len(text), chunk_size):\n",
        "        chunks.append(text[i:i+chunk_size])\n",
        "    return chunks\n",
        "\n",
        "documents = chunk_text(text)\n",
        "\n",
        "print(\"총 청크 수:\", len(documents))"
      ],
