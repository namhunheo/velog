        "query = \"RAG 단점은 뭐야?\"\n",
        "\n",
        "query_embedding = embed_model.encode([query])\n",
        "\n",
        "distances, indices = index.search(np.array(query_embedding), k=3)\n",
        "\n",
        "retrieved_docs = [documents[i] for i in indices[0]]\n",
        "\n",
        "print(\"검색된 PDF 내용:\")\n",
        "for doc in retrieved_docs:\n",
        "    print(\"-----\")\n",
        "    print(doc)"
