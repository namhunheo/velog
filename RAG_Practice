      "cell_type": "code",
      "source": [
        "documents = [                                          # <------------768 차원------------->\n",
        "    \"RAG는 검색과 생성이 결합된 기술입니다.\",          # [ 0.1    0.3    -0.12    0    0.05]\n",
        "    \"벡터 데이터베이스는 문서를 숫자로 저장합니다.\",\n",
        "    \"임베딩은 문장을 숫자로 변환하는 과정입니다.\",\n",
        "    \"파인튜닝은 모델을 추가 학습시키는 방법입니다.\"\n",
        "]"
      ],
      "metadata": {
        "id": "CLe7hPt1ru5Z"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "from sentence_transformers import SentenceTransformer\n",
        "import numpy as np\n",
        "\n",
        "embed_model = SentenceTransformer(\"jhgan/ko-sbert-nli\")\n",
        "\n",
        "doc_embeddings = embed_model.encode(documents)\n",
        "import faiss\n",
        "\n",
        "dimension = doc_embeddings.shape[1]\n",
        "\n",
        "index = faiss.IndexFlatL2(dimension)\n",
        "index.add(np.array(doc_embeddings))\n",
        "query = \"RAG가 무엇인가요?\"\n",
        "\n",
        "query_embedding = embed_model.encode([query])\n",
        "\n",
        "distances, indices = index.search(np.array(query_embedding), k=2)\n",
        "\n",
        "retrieved_docs = [documents[i] for i in indices[0]]\n",
        "\n",
        "print(\"검색된 문서:\")\n",
        "for doc in retrieved_docs:\n",
        "    print(\"-\", doc)"
      ],
