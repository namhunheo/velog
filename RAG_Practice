        "    **inputs,\n",
        "    max_new_tokens=100,\n",
        "    do_sample=True,\n",
        "    temperature=0.7\n",
        ")\n",
        "\n",
        "answer = tokenizer.decode(output[0], skip_special_tokens=True)\n",
        "\n",
        "print(answer)"
