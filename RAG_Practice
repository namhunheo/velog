        "import re\n",
        "\n",
        "def chunk_by_korean_bullets(text):\n",
        "    # 줄바꿈 정리\n",
        "    text = re.sub(r'\\n+', ' ', text)\n",
        "\n",
        "    # 가. 나. 다. 라. 등 기준으로 분할\n",
        "    chunks = re.split(r'(?=[가-힣].\\s)', text)\n",
        "\n",
        "    # 공백 제거 + 너무 짧은 항목 제거\n",
        "    chunks = [c.strip() for c in chunks if len(c.strip()) > 20]\n",
        "\n",
        "    return chunks\n",
        "\n",
        "\n",
        "documents = chunk_by_korean_bullets(text)\n",
        "\n",
        "print(\"총 청크 수:\", len(documents))\n",
        "\n",
        "for i, doc in enumerate(documents):\n",
        "    print(f\"\\n--- 청크 {i+1} ---\")\n",
        "    print(doc[:200])"
